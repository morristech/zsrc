#!/usr/bin/env gosh

(use gauche.parseopt)

(define (main args)
  (let ((sub-cmd (cadr args)))
    (let-args (cddr args)
              ((srcfile "f|file=s")
               . restargs)
              (print " sub-cmd: " sub-cmd)
              (print " srcfile: " srcfile)
              (print "restargs: " restargs))))
